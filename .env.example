# 랜덤 시크릿 생성: https://generate-secret.vercel.app/32 또는 `openssl rand -base64 32`
# 필수!
AUTH_SECRET=****

# 채팅 및 이미지 모델용 xAI API 키 받기: https://console.x.ai/
# 선택사항
XAI_API_KEY=****

# OpenAI API 키
# 선택사항
OPENAI_API_KEY=****

# Vercel Blob Store 생성 방법: https://vercel.com/docs/storage/vercel-blob
# 선택사항
BLOB_READ_WRITE_TOKEN=****

# PostgreSQL 데이터베이스 생성 방법: https://vercel.com/docs/storage/vercel-postgres/quickstart
# 로컬 개발 시 컨테이너 이름 대신 localhost 사용
# 필수!
POSTGRES_URL=postgresql://__REPLACE__LOCAL_DB_USER:__REPLACE__LOCAL_DB_PW@__REPLACE__LOCAL_DB_HOST:5432/__REPLACE__LOCAL_DB_NAME


# Redis 스토어 생성 방법:
# https://vercel.com/docs/redis
# 선택사항, 기본값은 undefined.
REDIS_URL=****

# 제목 생성 모델 (선택사항, 기본값은 사용 가능한 첫 번째 채팅 모델)
# 예시: gpt-3.5-turbo, gpt-4o-mini, claude-3-haiku-20240307
TITLE_GENERATION_MODEL=gpt-3.5-turbo

# 제안된 작업 숨기기 (선택사항, 기본값은 false)
# 채팅 인터페이스에서 제안된 작업을 숨기려면 "true"로 설정
NEXT_PUBLIC_HIDE_SUGGESTED_ACTIONS=false

# 기본 채팅 모델 (선택사항, 기본값은 사용 가능한 첫 번째 모델)
# 쿠키에서 모델이 선택되지 않았을 때 사용될 모델
DEFAULT_CHAT_MODEL=__REPLACE__CUSTOM_AI_MODEL

# 모델 선택기 UI 숨기기 (선택사항, 기본값은 false)
# 채팅 인터페이스에서 모델 선택기를 숨기려면 "true"로 설정
NEXT_PUBLIC_HIDE_MODEL_SELECTOR=false

# CXDM API 엔드포인트 (선택사항, 기본값은 프로덕션 URL)
CUSTOM_API_ENDPOINT=****

# 공개/비공개 선택기 UI 숨기기 (선택사항, 기본값은 false)
# 채팅 인터페이스에서 공개/비공개 선택기를 숨기려면 "true"로 설정
NEXT_PUBLIC_HIDE_VISIBILITY_SELECTOR=false

# 새 채팅의 기본 공개 설정 (선택사항, 기본값은 "private")
# 옵션: "private" 또는 "public"
DEFAULT_CHAT_VISIBILITY=private

# 사이드바 로고/홈 링크 숨기기 (선택사항, 기본값은 false)
# 사이드바에서 로고 링크를 숨기려면 "true"로 설정
NEXT_PUBLIC_HIDE_SIDEBAR_LOGO=false

# 메시지 작업에서 투표 버튼(좋아요/싫어요) 숨기기 (선택사항, 기본값은 false)
# 채팅 인터페이스에서 투표 버튼을 숨기려면 "true"로 설정
NEXT_PUBLIC_HIDE_VOTE_BUTTONS=false

# 채팅 입력창에서 첨부파일 버튼 숨기기 (선택사항, 기본값은 false)
# 채팅 인터페이스에서 첨부파일 버튼을 숨기려면 "true"로 설정
NEXT_PUBLIC_HIDE_ATTACHMENTS_BUTTON=false
